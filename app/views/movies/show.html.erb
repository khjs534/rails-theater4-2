<h1> <%= @movie.title %> </h1>
<% if logged_in? %>
  <%= link_to "Edit Movie", edit_movie_path(@movie) %>
  <%= link_to "Delete movie", movie_path(@movie), method: :delete %>
<% end %>
<ul>
  <% @movie.showings.each do |showing| %>
    <li>
      <%= link_to "#{showing.time}", showing_path(showing) %>
    </li>
    <% end %>
</ul>
